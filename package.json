    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "husky": "8.0.3",
    "postcss": "8.4.33",
    "prettier": "3.2.2",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3"
  },
  "lint-staged": {
    "./*/**/*.{js,jsx,ts,tsx}": [
      "npm run prettier:fix",
      "npm run eslint:fix"
